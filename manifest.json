{
  "name": "Darkblock Extension",
  "description": "Darkblock Chrome Browser Extension for Consuming Unlockable Content",
  "version": "0.2",
  "manifest_version": 3,
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["https://opensea.io/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}